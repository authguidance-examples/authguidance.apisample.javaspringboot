{
    "api": {
        "port": 8101,
        "sslCertificateFileName": "",
        "sslCertificatePassword": "",
        "useProxy": true,
        "proxyUrl": "http://127.0.0.1:8888",
        "webTrustedOrigins": [
            "https://web-garcher.eu.ngrok.io"
        ]
    },
    "logging": {
        "apiName": "SampleApi",
        "production": {
            "level": "info",
            "appenders": [
                {
                    "type": "console",
                    "prettyPrint": true
                },
                {
                    "type": "file",
                    "filePrefix": "api",
                    "dirname": "./logs",
                    "totalLogSizeMB": "1024",
                    "maxFiles": "100"
                }
            ],
            "performanceThresholdsMilliseconds": {
                "default": 250,
                "operationOverrides": {
                    "authorizer": 1000
                }
            }
        },
        "development": {
            "level": "info",
            "namespace": "com.mycompany.sample",
            "overrideLevels": {
                "ClaimsCache": "info"
            }
        }
    },
    "oauth": {
        "strategy":                      "standard",
        "issuer":                        "https://login-garcher.eu.ngrok.io/oauth/v2/oauth-anonymous",
        "audience":                      "api-garcher.eu.ngrok.io",
        "tokenValidationStrategy":       "jwt",
        "jwksEndpoint":                  "https://login-garcher.eu.ngrok.io/oauth/v2/oauth-anonymous/jwks",
        "introspectEndpoint":            "",
        "introspectClientId":            "",
        "introspectClientSecret":        "",
        "userInfoEndpoint":              "",
        "claimsCacheTimeToLiveMinutes":  0
    }
}
